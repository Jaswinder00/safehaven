{% extends 'base.html.twig' %}

{% block body %}
    <h1>ApplicationPerson list</h1>

    <table>
        <thead>
            <tr>
                <th>Isapplicant</th>
                <th>Name</th>
                <th>Age</th>
                <th>Hrsworked</th>
                <th>Isdisabled</th>
                <th>Isstudent</th>
                <th>Ismedicareentitled</th>
                <th>Isstateresident</th>
                <th>Isltcattest</th>
                <th>Isotherclaimed</th>
                <th>Isinsured</th>
                <th>Ispriorinsured</th>
                <th>Ispublicempbenefit</th>
                <th>Isfomerfoster</th>
                <th>Ispregnenant</th>
                <th>Islast3pregnant</th>
                <th>Isuscitizen</th>
                <th>Isincarcerated</th>
                <th>Istaxrequied</th>
                <th>Dod</th>
                <th>Numchildren</th>
                <th>Isclaimedoos</th>
                <th>Priorinsuranceenddate</th>
                <th>Hadfostermedicaid</th>
                <th>Ageleftfoster</th>
                <th>Fosterstatecd</th>
                <th>Lawllyfullypresent</th>
                <th>Immigrationstatus</th>
                <th>Iseligible</th>
                <th>Americanstatus</th>
                <th>Vetstatus</th>
                <th>Trafffickingvictim</th>
                <th>Immigrationdate</th>
                <th>Fiveyearbar</th>
                <th>Fiveyearbarmet</th>
                <th>Fortyquaters</th>
                <th>Immigrationdatetype</th>
                <th>Ffcapitalgains</th>
                <th>Capitalgains</th>
                <th>Mcmcapitalgains</th>
                <th>Agegt90</th>
                <th>Addressstreet</th>
                <th>Addresscity</th>
                <th>Addressstate</th>
                <th>Istempoos</th>
                <th>Nofixedaddress</th>
                <th>Createdtimestamp</th>
                <th>Nativeamerican</th>
                <th>Tobaccouser</th>
                <th>Addresszip</th>
                <th>Lastupdateuserid</th>
                <th>Lastupdatetimestamp</th>
                <th>Projectedamount</th>
                <th>Citstatnull</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for applicationPerson in applicationPeople %}
            <tr>
                <td><a href="{{ path('applicationperson_show', { 'id': applicationPerson.applicationid }) }}">{{ applicationPerson.isapplicant }}</a></td>
                <td>{{ applicationPerson.name }}</td>
                <td>{{ applicationPerson.age }}</td>
                <td>{{ applicationPerson.hrsworked }}</td>
                <td>{{ applicationPerson.isdisabled }}</td>
                <td>{{ applicationPerson.isstudent }}</td>
                <td>{{ applicationPerson.ismedicareentitled }}</td>
                <td>{{ applicationPerson.isstateresident }}</td>
                <td>{{ applicationPerson.isltcattest }}</td>
                <td>{{ applicationPerson.isotherclaimed }}</td>
                <td>{{ applicationPerson.isinsured }}</td>
                <td>{{ applicationPerson.ispriorinsured }}</td>
                <td>{{ applicationPerson.ispublicempbenefit }}</td>
                <td>{{ applicationPerson.isfomerfoster }}</td>
                <td>{{ applicationPerson.ispregnenant }}</td>
                <td>{{ applicationPerson.islast3pregnant }}</td>
                <td>{{ applicationPerson.isuscitizen }}</td>
                <td>{{ applicationPerson.isincarcerated }}</td>
                <td>{{ applicationPerson.istaxrequied }}</td>
                <td>{{ applicationPerson.dod }}</td>
                <td>{{ applicationPerson.numchildren }}</td>
                <td>{{ applicationPerson.isclaimedoos }}</td>
                <td>{% if applicationPerson.priorinsuranceenddate %}{{ applicationPerson.priorinsuranceenddate|date('Y-m-d') }}{% endif %}</td>
                <td>{{ applicationPerson.hadfostermedicaid }}</td>
                <td>{{ applicationPerson.ageleftfoster }}</td>
                <td>{{ applicationPerson.fosterstatecd }}</td>
                <td>{{ applicationPerson.lawllyfullypresent }}</td>
                <td>{{ applicationPerson.immigrationstatus }}</td>
                <td>{{ applicationPerson.iseligible }}</td>
                <td>{{ applicationPerson.americanstatus }}</td>
                <td>{{ applicationPerson.vetstatus }}</td>
                <td>{{ applicationPerson.trafffickingvictim }}</td>
                <td>{% if applicationPerson.immigrationdate %}{{ applicationPerson.immigrationdate|date('Y-m-d') }}{% endif %}</td>
                <td>{{ applicationPerson.fiveyearbar }}</td>
                <td>{{ applicationPerson.fiveyearbarmet }}</td>
                <td>{{ applicationPerson.fortyquaters }}</td>
                <td>{{ applicationPerson.immigrationdatetype }}</td>
                <td>{{ applicationPerson.ffcapitalgains }}</td>
                <td>{{ applicationPerson.capitalgains }}</td>
                <td>{{ applicationPerson.mcmcapitalgains }}</td>
                <td>{{ applicationPerson.agegt90 }}</td>
                <td>{{ applicationPerson.addressstreet }}</td>
                <td>{{ applicationPerson.addresscity }}</td>
                <td>{{ applicationPerson.addressstate }}</td>
                <td>{{ applicationPerson.istempoos }}</td>
                <td>{{ applicationPerson.nofixedaddress }}</td>
                <td>{% if applicationPerson.createdTimestamp %}{{ applicationPerson.createdTimestamp|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ applicationPerson.nativeamerican }}</td>
                <td>{{ applicationPerson.tobaccouser }}</td>
                <td>{{ applicationPerson.addresszip }}</td>
                <td>{{ applicationPerson.lastupdateUserid }}</td>
                <td>{{ applicationPerson.lastupdateTimestamp }}</td>
                <td>{{ applicationPerson.projectedAmount }}</td>
                <td>{{ applicationPerson.citStatNull }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('applicationperson_show', { 'id': applicationPerson.applicationid }) }}">show</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    
{% endblock %}
