<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="DCGov\HavenBundle\Entity\ApplicantRidp" table="applicant_ridp">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="applicantId" type="string" column="applicant_id" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="transId" type="string" column="trans_id" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="questionNo" type="smallint" column="question_no" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="questionValue" type="string" column="question_value" length="40" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="answer" type="text" column="answer" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="correctFlag" type="smallint" column="correct_flag" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
