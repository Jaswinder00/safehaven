<!-- Modal -->
<div id="incomeModal" class="modal fade" tabindex="-1" role="dialog">
	<div class="modal-dialog modal-lg" role="document">
		<!-- Modal content-->
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal">&times;</button>
				<h4 class="modal-title">Income Calculator</h4>
			</div>

			<div class="modal-body">
				<form id="frmCalculator" class="form-horizontal" action="#">
					<div class="row">
						<div class="col-xs-12 col-md-6">
							<div class="form-group cursor-pointer">
								<label class="col-md-6 control-label" for="incometype">Income Type:</label>
								<div class="col-md-6 form-inline">
									<select id="incometype" class="form-control" name="incometype">
										<option value="wages">Wages, salaries, tips, etc.</option>
										<option value=taxable_interest>Taxable interest</option>
										<option value="taxexempt_interest">Tax-exempt
											interest</option>
										<option value="ordinary_dividends">Ordinary dividends</option>
										<option value="taxable_refunds">Taxable refunds,
											credits, or offsets of ...</option>
										<option value="alimony_received">Alimony received</option>
										<option value="business_income">Business income (or
											loss)</option>
										<option value="capital_gains">Capital gain (or loss)</option>
										<option value="other_gains">Other gains (or losses)</option>
										<option value="ira_distributions">IRA distributions</option>
										<option value="pensions">Pensions and annuities</option>
										<option value="rental_income">Rental real-estate,
											royalites, partners ...</option>
										<option value="farm_income">Farm Income</option>
										<option value="unemployment_compensation">Unemployment
											compensation</option>
										<option value="ss_benefits">Social security benefits</option>
										<option value="taxable_ss_benefits">Taxable social
											security benefits</option>
										<option value="other_income">Other income</option>
									</select>
								</div>
							</div>
						</div>

						<div class="col-xs-12 col-md-6">
							<div class="form-group cursor-pointer">
								<label class="col-md-6 control-label" for="wagetype">Wage
									Type:</label>
								<div class="col-md-6 form-inline">
									<select id="wagetype" name="wagetype" class="form-control">
										<option value="w2wages">W2 Wages</option>
										<option value="1099wages">1099 Wages</option>
									</select>
								</div>
							</div>
						</div>
					</div>

					<!-- Employer Details -->
					<div id="rows_employerentries" class="well">
						<div class="row">
							<div class="form-group">
								<label class="col-md-3 control-label" for="employername">Employer Name:</label>
								<div class="col-md-3 form-inline">
									<input type="text" class="form-control cursor-pointer" id="employername"
										name="employername" placeholder="Employer Name">
								</div>
							
								<label class="col-md-3 control-label" for="employername">Employer ID Number:</label>
								<div class="col-md-3 form-inline">
									<input type="text" class="form-control cursor-pointer" id="employeridnumber"
										name="employeridnumber" placeholder="ID Number">
								</div>
							</div>
						</div>
						<div class="row">
							<div class="form-group">
								<label class="col-md-3 control-label" for="employeraddress">Employer
									Address:</label>
								<div class="col-md-3 form-inline">
									<input type="text" class="form-control cursor-pointer" id="employeraddress"
										name="employeraddress" placeholder="Employer Address">
								</div>
							
								<label class="col-md-3 control-label" for="employercity">Employer
									City:</label>
								<div class="col-md-3 form-inline">
									<input type="text" class="form-control cursor-pointer" id="employercity"
										name="employercity" placeholder="Employer City">
								</div>
							</div>
						</div>
						
						<div class="row">
							<div class="form-group">
								<label class="col-md-3 control-label" for="employerstate">Employer
									State:</label>
								<div class="col-md-3 form-inline">
									<select id="employerstate" name="employerstate"
										class="form-control cursor-pointer">
										<option value="">Select State</option>
										<option value="DC">District of Columbia</option>
										<option value="AL">Alabama</option>
										<option value="AK">Alaska</option>
										<option value="AZ">Arizona</option>
										<option value="AR">Arkansas</option>
										<option value="CA">California</option>
										<option value="CO">Colorado</option>
										<option value="CT">Connecticut</option>
										<option value="DE">Delaware</option>
										<option value="FL">Florida</option>
										<option value="GA">Georgia</option>
										<option value="HI">Hawaii</option>
										<option value="ID">Idaho</option>
										<option value="IL">Illinois</option>
										<option value="IN">Indiana</option>
										<option value="IA">Iowa</option>
										<option value="KS">Kansas</option>
										<option value="KY">Kentucky</option>
										<option value="LA">Louisiana</option>
										<option value="ME">Maine</option>
										<option value="MD">Maryland</option>
										<option value="MA">Massachusetts</option>
										<option value="MI">Michigan</option>
										<option value="MN">Minnesota</option>
										<option value="MS">Mississippi</option>
										<option value="MO">Missouri</option>
										<option value="MT">Montana</option>
										<option value="NE">Nebraska</option>
										<option value="NV">Nevada</option>
										<option value="NH">New Hampshire</option>
										<option value="NJ">New Jersey</option>
										<option value="NM">New Mexico</option>
										<option value="NY">New York</option>
										<option value="NC">North Carolina</option>
										<option value="ND">North Dakota</option>
										<option value="OH">Ohio</option>
										<option value="OK">Oklahoma</option>
										<option value="OR">Oregon</option>
										<option value="RI">Rhode Island</option>
										<option value="PA">Pennsylvania</option>
										<option value="SC">South Carolina</option>
										<option value="SD">South Dakota</option>
										<option value="TN">Tennessee</option>
										<option value="TX">Texas</option>
										<option value="UT">Utah</option>
										<option value="VT">Vermont</option>
										<option value="VA">Virginia</option>
										<option value="WA">Washington</option>
										<option value="WV">West Virginia</option>
										<option value="WI">Wisconsin</option>
										<option value="WY">Wyoming</option>
									</select>
								</div>

								<label class="col-md-3 control-label" for="employerzip">Employer
									Zip:</label>
								<div class="col-md-3 form-inline">
									<input type="text" class="form-control cursor-pointer" id="employerzip"
										name="employerzip" placeholder="Employer Zip">
								</div>
							</div>
						</div>
						
						<div class="row">
							<div class="form-group">
								<label class="col-md-3 control-label" for="isemployeeEligible">Is
									Employee Eligible:</label>
								<div class="col-md-3 form-inline">
									<select id="isemployeeEligible" name="isemployeeEligible"
										class="form-control cursor-pointer">
										<option value=""></option>
										<option value="Y">Yes</option>
										<option value="3">Within 3 Months</option>
										<option value="N">No</option>
									</select>
								</div>
							</div>
						</div>
					</div>

					<div id="rows_EmployeePlanDetails" class="well">
						<div class="row">
							<div class="form-group">
								<label class="col-md-3 control-label" for="employerplancoverage">*Employer
									Plan Covers:</label>
								<div class="col-md-3 form-inline">
									<select id="employerplancoverage" name="employerplancoverage"
										class="form-control cursor-pointer">
										<option value=""></option>
										<option value="emponly">Employee Only</option>
										<option value="family">Entire Family</option>
										<option value="empspouse">Employee &amp; Spouse</option>
										<option value="empdependent">Employee &amp;
											Dependents</option>
									</select>
								</div>
							
								<label class="col-md-3 control-label" for="planmeet">*Plan
									Meets Minimum:</label>
								<div class="col-md-3 form-inline">
									<select id="employerPlanMinimum" name="employerPlanMinimum"
										class="form-control cursor-pointer">
										<option value=""></option>
										<option value="Y">Yes</option>
										<option value="N">No</option>
									</select>
								</div>
							</div>
						</div>

						<div class="row">
							<div class="form-group">
								<label class="col-md-3 control-label" for="employeecost">*Employee
									Cost:</label>
								<div class="col-md-3 form-inline">
									<input type="number" step="any" class="form-control cursor-pointer" 
										id="employeecost" name="employeecost" placeholder="Employee Cost"/>
								</div>
							
								<label class="col-md-3 control-label" for="planfrequency">*Frequency:</label>
								<div class="col-md-3 form-inline">
									<select id="planfrequency" name="planfrequency"
										class="form-control">
										<option value="12">monthly</option>
										<option value="52">weekly</option>
										<option value="26">bi-weekly</option>
										<option value="6">bi-monthly</option>
										<option value="4">quarterly</option>
										<option value="2">half-yearly</option>
										<option value="1">annually</option>
										<option value="365">daily</option>
									</select>
								</div>
							</div>
						</div>
						
					</div>
					
					<div class="well">
    					<div class="row">
    						<div class="form-group">
    							<label class="col-md-3 control-label" for="paymentfrequency">*Payment
    								Frequency:</label>
    							<div class="col-md-3 form-inline">
    								<select id="paymentfrequency" name="paymentfrequency"
    									class="form-control cursor-pointer">
    									<option value="12">monthly</option>
    									<option value="52">weekly</option>
    									<option value="26">bi-weekly</option>
    									<option value="6">bi-monthly</option>
    									<option value="4">quarterly</option>
    									<option value="2">half-yearly</option>
    									<option value="1">annually</option>
    									<option value="365">daily</option>
    								</select>
    							</div>
    						
    							<label class="col-md-3 control-label" for="paymentamount">*Payment
    								Amount:</label>
    							<div class="col-md-3 form-inline">
    								<input type="number" step="any" class="form-control cursor-pointer"
    									id="paymentamount" name="paymentamount"
    									placeholder="Payment Amount" required>
    							</div>
    						</div>
    					</div>
    					
    					<div class="row">
    						<div class="form-group">
    							<label class="col-md-3 control-label" for="paystartdate">Start
    								Date:</label>
    							<div class="col-md-3 form-inline">
    								<input type="date" class="form-control" id="paystartdate"
    									name="paystartdate">
    							</div>
    							
    							<label class="col-md-3 control-label" for="payenddate">End
    								Date:</label>
    							<div class="col-md-3 form-inline">
    								<input type="date" class="form-control" id="payenddate"
    									name="payenddate">
    							</div>
    						</div>
    					</div>
    
    					<div class="row">
    						<div class="form-group">
    							<label class="col-md-3 control-label" for="annualincome">Annual
    								Income:</label>
    							<div class="col-md-3 form-inline">
    								<input type="number" step="any" class="form-control"
    									id="annualincome" name="annualincome" readonly>
    							</div>
    						
    							<label class="col-md-3 control-label" for="medicaid_current_monthly">Medicaid Current Monthly:</label>
    							<div class="col-md-3 form-inline">
    								<input type="number" step="any" class="form-control"
    									id="medicaid_current_monthly" name="medicaid_current_monthly"
    									readonly>
    							</div>
    						</div>
    					</div>
    					
					</div>
					
					<div class="row">
						<div class="col-md-6 col-md-offset-3 text-center">
							<div class="form-group ">
								<input type="submit" class="btn btn-danger btn-lg btn-block"
									value="Submit">
							</div>
						</div>
					</div>

				</form>
			</div>

			<div class="modal-footer">
				<button id="btnCalculatorClose" type="button"
					class="btn btn-default" data-dismiss="modal">Close</button>
			</div>
		</div>

	</div>
</div>