    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{% block title %}GREO Tools{% endblock %}</title>
    
   {% block stylesheets %}
	
	<!-- Bootstrap -->
    <link rel="stylesheet" href="{{ asset('css/bootstrap.css') }}">
    
    <!-- Datatable --->
    <link rel="stylesheet" href="{{ asset('css/datatable.css') }}">
	
	{% stylesheets 
		'@DCGovHavenBundle/Resources/public/css/signin.css'
		filter='cssrewrite'
	%}
    	<link rel="stylesheet" href="{{ asset_url }}" />
	{% endstylesheets %}
   {% endblock stylesheets %}
    
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    
    <script type="text/javascript" defer>
    	<!--- Initalize Javascript path variables ---!>
		var _route = "{{ app.request.attributes.get('_route') }}";
		
	</script> 
  
    {% block javascripts %}
        	
        	<!-- Include JQuery data table JS and CSS  -->
            <script type="text/javascript" src="{{ asset('js/jquery.js') }}" ></script>
            <script type="text/javascript" src="{{ asset('js/datatables.js') }}" defer></script>
            <script type="text/javascript" src="{{ asset('js/bootstrap.js') }}" defer></script>
            
            {% javascripts 	
            				'@DCGovHavenBundle/Resources/public/js/common.js'

            %}
            
            
        		<script src="{{ asset_url }}" defer></script>
            {% endjavascripts %}
            
	{% endblock javascripts %}
